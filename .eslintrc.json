{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/prefer-default-export": "off",
      "import/extensions": "off",
      "no-param-reassign": "off",
      "no-use-before-define": "off",
      "no-plusplus": "off"
    }
}
